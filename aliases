gs=git status  
gitref=explorer "http://gitref.org/"  
unfuck=git checkout .  
prune=git fetch --all --prune
git-feature=git checkout develop $t git pull $t git checkout -b feature/$1
git-bug=git checkout develop $t git pull $t git checkout -b bug/$1
git-hotfix=git checkout master $t git pull $t git checkout -b hotfix/$1
